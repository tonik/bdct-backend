{
  "name": "backend",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "yarn build:ts && tsc -p test/tsconfig.json && tap",
    "start": "yarn build:ts && fastify start -l info dist/app.js",
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "dev": "yarn build:ts && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
    "dev:start": "fastify start --ignore-watch=.ts$ -w -l info -P dist/app.js",
    "generate-oas": "ts-node utils/generateOas.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^5.0.0",
    "@fastify/sensible": "^5.0.0",
    "@fastify/swagger": "^8.8.0",
    "@fastify/swagger-ui": "^1.9.2",
    "fastify": "^4.0.0",
    "fastify-cli": "^5.7.1",
    "fastify-plugin": "^4.0.0",
    "fastify-zod": "^1.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.4.8",
    "@types/tap": "^15.0.5",
    "concurrently": "^7.0.0",
    "fastify-tsconfig": "^1.0.1",
    "tap": "^16.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.5.4"
  }
}
